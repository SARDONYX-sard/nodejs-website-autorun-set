{
  "name": "auto-web",
  "version": "1.0.0",
  "description": "auto-web",
  "keywords": [],
  "homepage": "https://github.com/SARDONYX-sard/nodejs-website-autorun-set",
  "bugs": "https://github.com/SARDONYX-sard/nodejs-website-autorun-set/issues",
  "license": "MIT",
  "author": "sard",
  "files": [
    "lib",
    "CHANGELOG.md",
    "CODE_OF_CONDUCT.md",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SARDONYX-sard/nodejs-website-autorun-set.git"
  },
  "main": "src/selenium/main.ts",
  "scripts": {
    "selenium": "npx ts-node src/selenium/custom/main.ts",
    "selenium-sample": "npx ts-node src/selenium/main.ts",
    "puppeteer": "npx ts-node src/puppeteer/main.ts",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint ./src/**/*.ts --fix",
    "build": "tsc",
    "test": "jest --coverage",
    "coverage": "codecov",
    "clean": "rm -rf ./src/selenium/custom/logs ./src/selenium/logs ./src/puppeteer/img/* ./coverage ./lib"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "chromedriver": "^91.0.1",
    "moment": "^2.29.1",
    "puppeteer": "^10.1.0",
    "selenium-webdriver": "^4.0.0-beta.4"
  },
  "devDependencies": {
    "@types/jest": "26.0.24",
    "@types/jest-plugin-context": "2.9.4",
    "@types/node": "15.12.4",
    "@types/selenium-webdriver": "4.0.15",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "codecov": "3.8.3",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "jest-plugin-context": "2.9.0",
    "lint-staged": "11.1.0",
    "prettier": "2.3.2",
    "ts-jest": "27.0.4",
    "ts-node": "10.1.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix ./src/**/*",
      "prettier --write ./src/**/*"
    ]
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=10.17"
  }
}
